package com.myspace.insurancepolicyrulestoringresponse;

import java.lang.Number;

rule "PassFinal Status"
	dialect "mvel"
	salience 1
	when
		$W : Insurance( statusAge == "Pass" && statusAnnualIncome == "Pass" && statusMedical_History == "Pass" && statusOccupation == "Pass" && statusPremiumAmount == "Pass" )
	then
		modify( $W ) {
				setStatusFinal( "Pass" )
		}
end
